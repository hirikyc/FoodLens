{
    "version": 2,
    "builds": [
        {
            "src": "app/app.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9",
                "buildCommand": "chmod +x build.sh && ./build.sh"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/app.py"
        }
    ]
} 
